{
  "manifest_version": 2,
  "name": "Zeplin Pixel to Rem",
  "short_name": "zeplinpxtorem",
  "version": "1.0",
  "browser_action": {
    "default_title": "Zeplin Pixel to Rem",
    "default_popup": "options.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://app.zeplin.io/*",
        "https://app.zeplin.io/*"
      ],
      "js": [
        "./dist/index.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}